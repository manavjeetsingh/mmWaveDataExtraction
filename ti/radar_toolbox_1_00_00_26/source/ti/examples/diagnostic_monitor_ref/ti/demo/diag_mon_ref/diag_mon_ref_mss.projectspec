        <file path="common/diag_mon_output.h" targetDirectory="common" openOnCreation="false" excludeFromBuild="false" action="copy"/>
        <file path="common/rfmonitor.c" openOnCreation="false" targetDirectory="common" excludeFromBuild="false" action="copy"/>
        <file path="common/rfmonitor_defaultCfg.c" openOnCreation="false" targetDirectory="common" excludeFromBuild="false" action="copy"/>
        <file path="common/mmwl_if.c" openOnCreation="false" targetDirectory="common" excludeFromBuild="false" action="copy"/>		
