        <file path="osal/src/tirtos/osal_DebugP_tirtos.c" openOnCreation="false" targetDirectory="osal/src/tirtos" excludeFromBuild="false" action="copy"/>
        <file path="osal/src/tirtos/osal_HwiP_tirtos.c" openOnCreation="false" targetDirectory="osal/src/tirtos" excludeFromBuild="false" action="copy"/>			
        <file path="osal/src/tirtos/osal_MemoryP_tirtos.c" openOnCreation="false" targetDirectory="osal/src/tirtos" excludeFromBuild="false" action="copy"/>	
        <file path="osal/src/tirtos/osal_r4f_esmdrv_tirtos.c" openOnCreation="false" targetDirectory="osal/src/tirtos" excludeFromBuild="false" action="copy"/>			
        <file path="osal/src/tirtos/osal_r4f_interrupt_tirtos.c" openOnCreation="false" targetDirectory="osal/src/tirtos" excludeFromBuild="false" action="copy"/>
        <file path="osal/src/tirtos/osal_SemaphoreP_tirtos.c" openOnCreation="false" targetDirectory="osal/src/tirtos" excludeFromBuild="false" action="copy"/>
        <file path="osal/MemoryP.h" targetDirectory="osal" openOnCreation="false" excludeFromBuild="false" action="copy"/>	
        <file path="osal/DebugP.h" targetDirectory="osal" openOnCreation="false" excludeFromBuild="false" action="copy"/>
